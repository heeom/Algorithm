    "score = input()\n",
    "\n",
    "n = len(score) // 2\n",
    "left = score[:n]\n",
    "right = score[n:]\n",
    "\n",
    "l,r = 0,0\n",
    "\n",
    "for i in range(n):\n",
    "    l += int(left[i])\n",
    "    r += int(right[i])\n",
    "    \n",
    "if l == r:\n",
    "    print('LUCKY')\n",
    "else:\n",
    "    print('READY')"
